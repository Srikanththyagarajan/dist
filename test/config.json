{
  "src_folders": [
    "test"
  ],
  "output_folder": "reports",
  "custom_commands_path": "",
  "custom_assertions_path": "",
  "page_objects_path": "",
  "globals_path": "",
  "selenium": {
    "start_process": false,
    "server_path": "../bin/selenium-server-standalone-3.3.1",
    "log_path": "",
    "port": 4444,
    "cli_args": {
      "webdriver.chrome.driver": "../bin/chromedriver_2.29",
      "webdriver.gecko.driver": "",
      "webdriver.edge.driver": ""
    }
  },
  "test_runner": {
    "type": "mocha",
    "options": {
      "ui": "bdd",
      "reporter": "list"
    }
  },
  "test_settings": {
    "default": {
      "launch_url": "http://localhost",
      "selenium_port": 4444,
      "selenium_host": "localhost",
      "silent": true,
      "screenshots": {
        "enabled": false,
        "path": ""
      },
      "desiredCapabilities": {
        "browserName": "firefox",
        "marionette": true
      }
    },
    "chrome": {
      "desiredCapabilities": {
        "browserName": "chrome",
        "javascriptEnabled": true,
        "acceptSslCerts": true,
        "chromeOptions": {
          "args": [
            "window-size=1880,800",
            "disable-web-security"
          ]
        }
      }
    },
    "edge": {
      "desiredCapabilities": {
        "browserName": "MicrosoftEdge"
      }
    }
  }
}
